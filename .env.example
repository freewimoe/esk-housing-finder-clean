# API Configuration Example
# Copy this file to .env and add your actual API keys

# Google APIs
GOOGLE_PLACES_API_KEY=your_google_places_api_key_here
GOOGLE_GEOCODING_API_KEY=your_google_geocoding_api_key_here

# Weather API
OPENWEATHER_API_KEY=your_openweather_api_key_here

# Economic Data
FRED_API_KEY=your_fred_api_key_here

# Real Estate APIs (optional)
ZILLOW_API_KEY=your_zillow_api_key_here
RENTSPIDER_API_KEY=your_rentspider_api_key_here

# Database (if using)
DATABASE_URL=postgresql://user:password@localhost:5432/real_estate_db

# App Settings
DEBUG=True
LOG_LEVEL=INFO
